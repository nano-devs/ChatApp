{
  "ConnectionStrings": {
    "DataSQLiteConnection": "Data Source=ChatAppData.db;",
    "DataPostgreSQLConnection": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "HttpsFromPem": {
        "Url": "https://localhost:5001",
        "Certificate": {
          "Path": "../localhost.pem",
          "KeyPath": "../localhost-key.pem"
        }
      }
    }
  }
}
